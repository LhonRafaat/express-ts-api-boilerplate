{
  "entryFile": "./src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/**/*controller.ts"],
  "spec": {
    "outputDirectory": "src/docs",
    "specVersion": 3,
    "basePath": "/api/v1",
    "securityDefinitions": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "bearerFormat"
      }
    }
  },
  "routes": {
    "basePath": "/api/v1",
    "routesDir": "src/docs"
  }
    
}
